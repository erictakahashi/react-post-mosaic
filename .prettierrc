{
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 80,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "arrowParens": "always",
    "jsxSingleQuote": false,
    "organizeImportsSkipDestructiveCodeActions": true,
    "plugins": [
        "prettier-plugin-organize-imports"
    ]
}